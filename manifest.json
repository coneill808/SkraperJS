{
    "name": "Skraper",
    "description": "Lightweight chrome extension for downloading data from tables",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions":["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/skraper_icon.png"
        }
    },
    "icons": {
        "128":"/images/skraper_icon.png"
    }
}